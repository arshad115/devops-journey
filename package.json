{
  "name": "express",
  "version": "1.0.0",
  "description": "Express API ",
  "scripts": {
    "watch": "nodemon --exec mocha",
    "prebuild": "npm run clean",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "ts-node src/server.ts",
    "start:dev": "nodemon src/server.ts",
    "test": "mocha",
    "coverage": "c8 --check-coverage --lines 95 --functions 95 --branches 90 --statements 95 --reporter=lcov --reporter=text mocha",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kookydroid-apps/express.git"
  },
  "author": "Kookydroid Apps",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kookydroid-apps/express/issues"
  },
  "homepage": "https://github.com/kookydroid-apps/express#readme",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.7.0",
    "@types/supertest": "^6.0.2",
    "c8": "^10.1.2",
    "express": "^4.21.0",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.7",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": "^20",
    "npm": ">=9"
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "spec": [
      "test/**/*.test.ts"
    ],
    "require": "ts-node/register"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "test"
    ],
    "ext": "js,json,ts"
  }
}
